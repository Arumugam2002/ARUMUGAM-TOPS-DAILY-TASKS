import java.util.HashMap;
import java.util.Map;

public class MapExample {

	public static void main(String[] args)
	{
		Map map = new HashMap();
		
		map.put(1, "c");
		
		map.put(2, "c++");
		map.put("java", 3);
		map.put(1.4, "php");
		map.put(true, "python");
		map.put(null, "java");
		
		System.out.println(map);
		
	}
	
}
